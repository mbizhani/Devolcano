<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE plan PUBLIC
		"Plan"
		"http://www.devocative.org/dtd/devolcano-plan.dtd">

<plan>
	<pre>
		<![CDATA[
			def params = [
				moduleShortName : "mts",
				ajaxEditColumn : false,
				iconClass : org.devocative.demeter.web.DemeterIcon
			]
		]]>
	</pre>

	<packageMap>
		<from pkg="org.devocative.devolcano.test.entity" includeSubPackages="false">
			<!--<to pkgReplace="entity:vo" generatorRef="LVO" genDir="src/test/java"/>-->
			<to pkgReplace="entity:vo" generatorRef="FVO" genDir="src/test/java"/>

			<to pkgReplace="entity:iservice" generatorRef="ServiceI" genDir="src/test/java"/>
			<to pkgReplace="entity:service" generatorRef="ServiceM" genDir="src/test/java"/>

			<to pkgReplace="entity:web" generatorRef="ListH" genDir="src/test/java"/>
			<to pkgReplace="entity:web" generatorRef="ListJ" genDir="src/test/java"/>

			<to pkgReplace="entity:web" generatorRef="FormH" genDir="src/test/java"/>
			<to pkgReplace="entity:web" generatorRef="FormJ" genDir="src/test/java"/>
		</from>
	</packageMap>

	<volcanoes>
		<volcano name="FVO">
			<precondition>
				<![CDATA[
				targetClass.normal && targetClass.ok
				]]>
			</precondition>
			<template file="/templates/FVO.gsp"
					  suffix="FVO"
					  genFileType="java"
					  overwrite="check">
				<overwriteCheckString>//overwrite</overwriteCheckString>
			</template>
		</volcano>

		<volcano name="ServiceI">
			<precondition>
