<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE meta PUBLIC
		"Meta Data"
		"http://www.devocative.org/dtd/devolcano-metadata.dtd">

<meta>
	<filterClass>targetClass.entity</filterClass>
	<classes>
		<class fqn="org.devocative.devolcano.test.entity.Person">
			<cinfo/>
			<id ref="id"/>
			<fields>
				<field name="id">
					<finfo/>
				</field>
				<field name="name">
					<finfo/>
				</field>
				<field name="birthDate">
					<finfo/>
				</field>
				<field name="books">
					<finfo required="true"/>
				</field>
				<field name="weight">
					<finfo hasForm="false" hasFVO="false"/>
				</field>
			</fields>
		</class>
		<class fqn="org.devocative.devolcano.test.entity.Book">
			<cinfo/>
			<id ref="id"/>
			<fields>
				<field name="id">
					<finfo/>
				</field>
				<field name="name">
					<finfo/>
				</field>
				<field name="description">
					<finfo textType="multiline">
						<htmlAttr>cols=&quot;40&quot; rows=&quot;10&quot;</htmlAttr>
					</finfo>
				</field>
				<field name="sampleCode">
					<finfo textType="code" codeType="XML"/>
				</field>
				<field name="author">
					<finfo/>
				</field>
				<field name="publishYear">
					<finfo/>
				</field>
				<field name="bookType">
					<finfo/>
				</field>
			</fields>
		</class>
	</classes>
</meta>